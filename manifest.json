{
    "manifest_version": 3,
    "name": "TabCycle",
    "version": "1.1.0",
    "description": "Cycles through open tabs at a set time interval.",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "TabCycle",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "web_accessible_resources": [
        {
        "resources": ["styles.css"],
        "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
  }
}